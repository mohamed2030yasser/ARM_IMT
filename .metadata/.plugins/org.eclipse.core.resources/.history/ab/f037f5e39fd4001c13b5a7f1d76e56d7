#include <avr/io.h>
#include <util/delay.h>

int main()
{
	int x;
	             // 76543210;
	DDRA =0b11111101;
	PORTA=0b00000010; // active polup



	while(1)
	{
		if((PINA&&(1<<1))==0)
		{
			PORTA=0b00000011;
		}else
				{
					PORTA=0b00000010;
				}


	}



}
