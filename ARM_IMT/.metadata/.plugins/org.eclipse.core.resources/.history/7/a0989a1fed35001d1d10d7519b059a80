
#include "LIB/STD_TYPES.h"
#include "LIB/BIT_MATH.h"
#include "MCAL/RCC/RCC_int.h"
#include "MCAL/GPIO/GPIO_int.h"

// FORBIDDEN PINS 		[A13 A14 A15] : [B2 B3 B4]

int main(void)
{
	int x =10000;
	MRCC_vInit();
	MRCC_vEnClock(RCC_AHB1, RCC_EN_GPIOA);

	for (int i = 0; i < 7; i++) {
		GPIO_vSetPinMod( GPIO_PORTA, i, GPIO_MODE_GPOUT);
		GPIO_vSetPinOutSpeed(GPIO_PORTA, i, GPIO_LOW_SPEED);
		GPIO_vSetPinOutType(GPIO_PORTA, i, GPIO_PUSHPULL);
	}

/*
for (int i = 0; i < 7; i++) {
		GPIO_vSetPinValue(GPIO_PORTA, i, GET_BIT(0b0000010,i));
		//while(x--);
	}
 */
	GPIO_vSetPinValue(GPIO_PORTA, 0, 0);
	GPIO_vSetPinValue(GPIO_PORTA, 1, 1);
	GPIO_vSetPinValue(GPIO_PORTA, 2, 0);
	GPIO_vSetPinValue(GPIO_PORTA, 3, 0);
	GPIO_vSetPinValue(GPIO_PORTA, 4, 0);
	GPIO_vSetPinValue(GPIO_PORTA, 5, 0);
	GPIO_vSetPinValue(GPIO_PORTA, 6, 0);








	while(1)
	{

	}
}
