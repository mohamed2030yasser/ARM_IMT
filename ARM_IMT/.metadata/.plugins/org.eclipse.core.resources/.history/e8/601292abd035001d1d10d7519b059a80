
#include "LIB/STD_TYPES.h"
#include "MCAL/RCC/RCC_int.h"
#include "MCAL/GPIO/GPIO_int.h"


int main(void)
{
	MRCC_vInit();
	MRCC_vEnClock(RCC_AHB1, RCC_EN_GPIOA);
	MRCC_vEnClock(RCC_AHB1, RCC_EN_GPIOB);

	GPIO_vSetPinMod( GPIO_PORTB, 0, GPIO_MODE_GPOUT);
	GPIO_vSetPinOutSpeed(GPIO_PORTB, 0, GPIO_LOW_SPEED);
	GPIO_vSetPinOutType(GPIO_PORTB, 0, GPIO_PUSHPULL);

	GPIO_vSetPinMod( GPIO_PORTB, 1, GPIO_MODE_GPOUT);
	GPIO_vSetPinOutSpeed(GPIO_PORTB, 1, GPIO_LOW_SPEED);
	GPIO_vSetPinOutType(GPIO_PORTB, 1, GPIO_PUSHPULL);

	GPIO_vSetPinMod( GPIO_PORTA, 7, GPIO_MODE_GPOUT);
	GPIO_vSetPinOutSpeed(GPIO_PORTA, 7, GPIO_LOW_SPEED);
	GPIO_vSetPinOutType(GPIO_PORTA, 7, GPIO_PUSHPULL);


	//GPIO_vSetPinValue(GPIO_PORTB, 0, GPIO_HIGH);
	//GPIO_vSetPinValue(GPIO_PORTB, 1, GPIO_HIGH);
	GPIO_vSetPinValue(GPIO_PORTA, 7, GPIO_HIGH);
	//GPIO_vSetPortValue(GPIO_PORTA, 4);

	while(1)
	{

	}
}
